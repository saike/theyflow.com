{
  "name": "theyflow.com",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "node_modules/.bin/nodemon -L --exec babel-node server/server.js",
    "prod": "node_modules/.bin/pm2 start --interpreter babel-node server/server.js",
    "restart": "node_modules/.bin/pm2 restart --interpreter babel-node server",
    "stop": "node_modules/.bin/pm2 stop --interpreter babel-node server",
    "migrate": "babel-node ./node_modules/.bin/mm migrate --config=server/config/mongodb.js"
  },
  "keywords": [
    "theyflow"
  ],
  "author": "saike <kishkoglot@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "nodemon": "^1.11.0",
    "pm2": "^2.6.1"
  }
}
